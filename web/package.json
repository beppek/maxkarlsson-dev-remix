{
  "name": "maxkarlsson.dev",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && cross-env NODE_ENV=production remix build",
    "build:css": "tailwindcss -i ./styles/global.css -o ./app/tailwind.css --minify",
    "dev": "concurrently \"npm run dev:css\" \"cross-env NODE_ENV=development run-p dev:*\"",
    "dev:css": "tailwindcss -i ./styles/global.css -o ./app/tailwind.css --watch",
    "postinstall": "remix setup cloudflare-pages",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public",
    "start": "npm run dev:wrangler",
    "dev:remix": "remix watch"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.4",
    "@heroicons/react": "^2.0.13",
    "@remix-run/cloudflare": "^1.8.0",
    "@remix-run/cloudflare-pages": "^1.8.0",
    "@remix-run/react": "^1.8.0",
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/image-url": "^1.0.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "get-youtube-id": "^1.0.1",
    "picosanity": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-inlinesvg": "^3.0.1",
    "react-refractor": "^2.1.7",
    "react-youtube": "^9.0.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@remix-run/dev": "^1.8.0",
    "@remix-run/eslint-config": "^1.8.0",
    "@tailwindcss/typography": "^0.5.8",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/refractor": "^3.0.2",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "eslint": "^8.28.0",
    "groq": "^2.33.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "wrangler": "2.5.0"
  },
  "engines": {
    "node": ">=14"
  }
}
