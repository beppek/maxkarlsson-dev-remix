    "test": "sanity check",
    "export": "sanity dataset export production ./exports && npm run extract",
    "export:staging": "sanity dataset export staging ./exports && npm run extract:staging",
    "extract": "tar -xzvf ./exports/production.tar.gz -C ./exports",
    "extract:staging": "tar -xzvf ./exports/staging.tar.gz -C ./exports",
    "deploy:graphql": "sanity graphql deploy --playground",
    "build": "sanity build public -y",
    "deploy": "SANITY_ACTIVE_ENV=production sanity deploy"
